    "redux-devtools-extension": "^2.13.7",
    "reza": "^1.0.15",
    "reza-plugin-scss": "^1.0.2",
    "rimraf": "^2.6.3",
    "supertest": "^3.0.0"
  },
  "_moduleAliases": {
    "@app": "src",
    "@config": "src/server/config",
    "@api": "src/server/api",
    "@middlewares": "src/server/middlewares",
    "@models": "src/server/models",
    "@services": "src/server/services",
    "@utils": "src/server/utils",
    "@routes": "src/server/routes",
    "@client": "src/client",
    "@components": "src/client/components",
    "@containers": "src/client/containers",
    "@ducks": "src/client/ducks",
    "@hox": "src/client/hox"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "**/index.js"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/src$1",
      "^@boot(.*)$": "<rootDir>/src/server/boot$1",
      "^@config(.*)$": "<rootDir>/src/server/config$1",
      "^@api(.*)$": "<rootDir>/src/server/api$1",
      "^@middlewares(.*)$": "<rootDir>/src/server/middlewares$1",
      "^@models(.*)$": "<rootDir>/src/server/models$1",
      "^@services(.*)$": "<rootDir>/src/server/services$1",
      "^@utils(.*)$": "<rootDir>/src/server/utils$1",
      "^@routes(.*)$": "<rootDir>/src/server/routes$1",
      "^@client(.*)$": "<rootDir>/src/client$1",
      "^@components(.*)$": "<rootDir>/src/client/components$1",
      "^@containers(.*)$": "<rootDir>/src/client/containers$1",
      "^@ducks(.*)$": "<rootDir>/src/client/ducks$1",
      "^@hox(.*)$": "<rootDir>/src/client/hox$1"
    },
    "setupFiles": [
      "<rootDir>/test.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverageFrom": [
      "src/**/*{js,jsx}",
      "!src/**/*{epics}.js",
      "!src/**/*{reducers}.js",
      "!src/**/*{store}.js",
      "!src/**/*{routes|route}.js",
      "!src/**/*{actionTypes}.js",
      "!src/**/index.js",
      "!src/**/*.validation.js",
      "!src/**/*.validator.js",
      "!src/server/config/**",
      "!src/server/database/**",
      "!src/server/models/**",
      "!src/server/utils/ErrorCode/**",
      "!src/server/services/http/**",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text",
      "clover"
    ]
  }
}
