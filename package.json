{
  "name": "cms-admin-poc",
  "version": "1.0.0",
  "description": "This project generated using Astro Generator",
  "author": "Rahul Khanna",
  "main": "src/server/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "precommit": "yarn validate",
    "prestart": "yarn docs",
    "build": "reza build",
    "start": "rimraf dist/ && npm run build && cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "rimraf dist/ && cross-env NODE_ENV=development reza start",
    "lint": "eslint '**/*.js' --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "test": "cross-env NODE_ENV=test reza test --env=jsdom --forceExit --detectOpenHandles --coverage  --coverageDirectory=coverage --testPathPattern=\"(/src/.*)\\.(test|spec).js$\"",
    "test:unit": "cross-env NODE_ENV=test reza test --env=jsdom --forceExit --detectOpenHandles --testPathPattern=\"(/src/.*)\\.(spec).js$\"",
    "test:integration": "cross-env NODE_ENV=test reza test --env=jsdom --forceExit --detectOpenHandles --testPathPattern=\"(/src/.*)\\.(test).js$\"",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/server/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "jest",
    "eslint",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project",
    "aws"
  ],
  "dependencies": {
